{
    "presets":[
        "@babel/env",
        "@babel/react"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-class-properties",
            { "loose": true }
        ],
        "@babel/plugin-syntax-dynamic-import",
        [
            "module-resolver",
            {
                "extensions": [".js", ".jsx", ".ts", ".tsx", ".mjs"],
                "alias": {
                    "@src": "./src"
                }
            }
        ]
    ]
}
